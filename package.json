{
  "name": "QiniuClient",
  "version": "0.0.1",
  "author": "Jinsihou",
  "description": "A third client for qiniu client",
  "scripts": {
    "pack": "build --dir",
    "dist": "npm run dist:osx",
    "dist:osx": "build --x64 -m --dir",
    "dist:win": "build --platform win32 --arch all -d",
    "dist:linux": "build --platform linux --arch all -d"
  },
  "keywords": [
    "qiniu"
  ],
  "homepage": "http://github.jinsihou19.io",
  "devDependencies": {
    "electron-builder": "latest",
    "electron-prebuilt": "latest"
  },
  "build": {
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    },
    "dmg": {
      "title": "QiniuClient",
      "background": "build/background.png",
      "icon": "build/icon.icns",
      "icon-size": 128,
      "appId": "jinsihou.qiniu",
      "app-category-type": "public.app-category.productivity",
      "contents": [
        {
          "x": 355,
          "y": 125,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 155,
          "y": 125,
          "type": "file"
        }
      ]
    }
  }
}